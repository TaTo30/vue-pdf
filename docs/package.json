{
  "name": "vue-pdf-docs",
  "version": "1.0.0",
  "description": "vue-pdf docs",
  "scripts": {
    "dev": "vitepress dev .",
    "build": "npm run patch && vitepress build --clean-temp --clean-cache .",
    "patch": "npm run patch:legacy",
    "patch:minimal": "move-file ../packages/vue-pdf/dist/index.minimal.mjs ../packages/vue-pdf/dist/index.mjs",
    "patch:legacy": "replace-in-file --configFile=config.json ../packages/vue-pdf/dist/index.mjs"
  },
  "dependencies": {
    "@tato30/vue-pdf": "*"
  },
  "devDependencies": {
    "move-file-cli": "^3.0.0",
    "replace-in-file": "^8.3.0",
    "sass": "^1.77.2",
    "vite": "^7.1.9",
    "vitepress": "^1.6.4"
  }
}
